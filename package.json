{
  "name": "ng-es2015-webpack",
  "version": "1.0.0-beta",
  "description": "Seed project for frontend development with PostCSS for CSS, ES2015 for Javascript, Webpack for dependency management, and Angular with Karma+Jasmine for a development framework.",
  "main": "dist/main.js",
  "scripts": {
    "build": "gulp build",
    "build-production": "gulp build-production",
    "clean": "gulp clean",
    "coverage-ci": "npm run test-unit && cat dist/coverage/lcov/lcov.info | node node_modules/coveralls/bin/coveralls.js",
    "develop": "gulp develop",
    "documentation": "jsdoc -R README.md -P package.json -r src -d dist/documentation",
    "generate-component": "gulp generate:component",
    "generate-directive": "gulp generate:directive",
    "generate-service": "gulp generate:service",
    "lint-css": "stylelint \"src/**/*.css\"",
    "lint-js": "eslint \"src/**/*.js\"",
    "postinstall": "npm run setup-tasks && npm run setup-protractor",
    "setup-tasks": "rollup -c tasks/rollup.config.js",
    "setup-protractor": "webdriver-manager update",
    "start": "gulp production",
    "test": "jasmine package.e2e.js",
    "test-unit": "cross-env BABEL_ENV=test gulp test",
    "test-unit-dev": "cross-env BABEL_ENV=test gulp watch:test",
    "test-integration": "protractor tasks/test/protractor.config.js"
  },
  "author": "Giuseppe Scoppino <scoppino.giuseppe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "~6.24.x",
    "babel-loader": "~7.0.x",
    "babel-plugin-external-helpers": "~6.22.x",
    "babel-plugin-istanbul": "~4.1.x",
    "babel-polyfill": "~6.23.x",
    "babel-preset-es2015": "~6.24.x",
    "browser-sync": "~2.18.x",
    "connect-history-api-fallback": "~1.3.x",
    "coveralls": "~2.13.x",
    "critical": "~0.8.x",
    "cross-env": "~4.0.x",
    "cssnano": "~3.10.x",
    "del": "~2.2.x",
    "eslint": "~3.19.x",
    "eslint-loader": "~1.7.x",
    "gulp": "~3.9.x",
    "gulp-htmlmin": "~3.0.x",
    "gulp-rename": "~1.2.x",
    "gulp-template": "~4.0.x",
    "jasmine": "~2.5.x",
    "jsdoc": "~3.4.x",
    "karma": "~1.6.x",
    "karma-coverage": "~1.1.x",
    "karma-jasmine": "~1.1.x",
    "karma-phantomjs-launcher": "~1.0.x",
    "karma-sourcemap-loader": "~0.3.x",
    "karma-webpack": "~2.0.x",
    "phantomjs-prebuilt": "~2.1.x",
    "postcss": "~5.2.x",
    "postcss-cssnext": "~2.10.x",
    "postcss-import": "~9.1.x",
    "postcss-reporter": "~3.0.x",
    "protractor": "~5.1.x",
    "raw-loader": "~0.5.x",
    "rollup": "~0.41.x",
    "rollup-plugin-babel": "~2.7.x",
    "stylelint": "~7.10.x",
    "stylelint-config-standard": "~16.0.x",
    "sw-precache-webpack-plugin": "~0.10.0",
    "systemjs": "~0.20.x",
    "systemjs-plugin-babel": "~0.0.17",
    "webpack": "~2.4.x",
    "yargs": "~7.1.x"
  },
  "dependencies": {
    "angular": "~1.6.x",
    "angular-aria": "~1.6.x",
    "angular-mocks": "~1.6.x"
  }
}
