{
  "name": "ng-es2015-webpack",
  "version": "1.0.0-beta",
  "description": "Seed project for frontend development with PostCSS for CSS, ES2015 for Javascript, Webpack for dependency management, and Angular with Karma+Jasmine for a development framework.",
  "main": "dist/main.js",
  "scripts": {
    "build:builder": "rollup -c gulp/rollup.config.js",
    "postinstall": "npm run build:builder",
    "clean": "gulp clean",
    "build": "gulp build",
    "build-production": "gulp build-production",
    "develop": "gulp develop",
    "production": "gulp production",
    "test": "cross-env BABEL_ENV=test gulp test",
    "test-ci": "cross-env BABEL_ENV=test gulp watch:test",
    "doc-generator": "jsdoc -R README.md -P package.json -r src -d dist/documentation",
    "generate:directive": "gulp generate:directive",
    "generate:component": "gulp generate:component",
    "generate:service": "gulp generate:service",
    "generate:route": "gulp generate:route"
  },
  "author": "Giuseppe Scoppino <scoppino.giuseppe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "~6.16.x",
    "babel-loader": "~6.2.x",
    "babel-plugin-external-helpers": "~6.8.x",
    "babel-plugin-istanbul": "~2.0.x",
    "babel-polyfill": "~6.16.x",
    "babel-preset-es2015": "~6.16.x",
    "browser-sync": "~2.17.x",
    "connect-history-api-fallback": "~1.3.x",
    "cross-env": "~3.1.x",
    "cssnano": "~3.7.x",
    "del": "~2.2.x",
    "eslint": "~3.8.x",
    "eslint-loader": "~1.6.x",
    "gulp": "~3.9.x",
    "gulp-htmlmin": "~3.0.x",
    "gulp-inject": "~4.1.x",
    "gulp-postcss": "~6.2.x",
    "gulp-rename": "~1.2.x",
    "gulp-template": "~4.0.x",
    "jasmine-core": "~2.5.x",
    "jsdoc": "~3.4.x",
    "karma": "~1.3.x",
    "karma-coverage": "~1.1.x",
    "karma-jasmine": "~1.0.x",
    "karma-phantomjs-launcher": "~1.0.x",
