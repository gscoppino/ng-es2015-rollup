{
  "name": "ng-es2015-rollup",
  "version": "0.1.0",
  "description": "Seed project for frontend development with PostCSS for CSS, ES2015 for Javascript, Rollup for dependency management, and Angular with Karma+Jasmine for a development framework.",
  "main": "package.json",
  "scripts": {
    "build:builder": "rollup -c gulp/rollup.config.js",
    "postinstall": "npm run build:builder",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "build": "gulp build",
    "predevelop": "npm run build",
    "develop": "gulp develop",
    "test": "gulp test",
    "test-ci": "gulp watch:test"
  },
  "author": "Giuseppe Scoppino <scoppino.giuseppe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "~6.9.x",
    "babel-eslint": "~6.0.x",
    "babel-istanbul": "~0.8.x",
    "babel-plugin-transform-class-properties": "~6.9.x",
    "babel-polyfill": "~6.9.x",
    "babel-preset-es2015-rollup": "~1.1.x",
    "browser-sync": "~2.12.x",
    "cash-rm": "~0.2.x",
    "connect-history-api-fallback": "~1.2.x",
    "gulp": "~3.9.x",
    "jasmine-core": "~2.4.x",
    "karma": "~0.13.x",
    "karma-coverage": "~1.0.x",
    "karma-jasmine": "~1.0.x",
    "karma-phantomjs-launcher": "~1.0.x",
    "karma-rollup-preprocessor": "~2.0.x",
    "karma-sourcemap-loader": "~0.3.x",
    "phantomjs-prebuilt": "~2.1.x",
    "postcss": "~5.0.x",
    "postcss-cssnext": "~2.5.x",
    "postcss-import": "~8.1.x",
    "postcss-reporter": "~1.3.x",
    "rollup": "~0.26.x",
    "rollup-plugin-babel": "~2.4.x",
    "rollup-plugin-commonjs": "~2.2.x",
    "rollup-plugin-eslint": "~2.0.x",
    "rollup-plugin-istanbul": "~1.0.x",
    "rollup-plugin-node-resolve": "~1.5.x",
    "rollup-plugin-string": "~1.0.x",
    "stylelint": "~6.5.x",
    "stylelint-config-standard": "~8.0.x"
  },
  "dependencies": {
    "angular": "~1.5.x",
    "angular-mocks": "~1.5.x"
  }
}
